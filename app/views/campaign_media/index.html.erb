<p id="notice"><%= notice %></p>

<h1>Listing Campaign Media</h1>

<table>
  <thead>
    <tr>
      <th>Medium</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @campaign_media.each do |campaign_medium| %>
      <tr>
        <td><%= campaign_medium.medium %></td>
        <td><%= link_to 'Show', campaign_medium %></td>
        <td><%= link_to 'Edit', edit_campaign_medium_path(campaign_medium) %></td>
        <td><%= link_to 'Destroy', campaign_medium, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Campaign medium', new_campaign_medium_path %>
