<!-- BEGIN PORTLET-->
<div class="portlet light bordered">
    <div class="portlet-title">
        <div class="caption">
            <i class="icon-bar-chart font-green"></i>
            <span class="caption-subject font-green bold uppercase">網站狀況</span>
            <span class="caption-helper"></span>
        </div>
        <div class="actions">
            <div class="btn-group btn-group-devided" data-toggle="buttons">
                <label class="btn red btn-outline btn-circle btn-sm active">
                    <input type="radio" name="graphs1" class="toggle" id="7days" onclick="_7Daysonclicked()">7日</label>
                <label class="btn red btn-outline btn-circle btn-sm">
                    <input type="radio" name="graphs1" class="toggle" id="30days" onclick="_30Daysonclicked()">30日</label>
            </div>
        </div>
    </div>
    <script>
    $(document).ready(function() {
      $('input[type=radio][name=graphs1]').change(function() {
          if (this.id == '7days') {
            $('#30DaysData').hide();
            $('#7DaysData').show();
          } else if (this.id == '30days') {
            $('#7DaysData').hide();
            $('#30DaysData').show();
          }
      });
    });
    </script>
    <div class="portlet-body">

      <div id="7DaysData" class="row" style="margin:0">
          <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
              <div class="dashboard-stat2 ">
                  <div class="display">
                      <div class="number">
                          <h3 class="font-green-sharp">
                              <span data-counter="counterup" data-value="<%= @grpah1Data7[:sessions] %>">0</span>
                              <small class="font-green-sharp"><!--$--></small>
                          </h3>
                          <small>Sessions</small>
                      </div>
                      <div class="icon">
                          <i class="icon-pie-chart"></i>
                      </div>
                  </div>
                  <div class="progress-info">
                      <div class="progress">
                          <span style="width: 76%;" class="progress-bar progress-bar-success green-sharp">
                              <span class="sr-only">76% progress</span>
                          </span>
                      </div>
                      <div class="status">
                          <div class="status-title"> progress </div>
                          <div class="status-number"> 76% </div>
                      </div>
                  </div>
              </div>
          </div>
          <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
              <div class="dashboard-stat2 ">
                  <div class="display">
                      <div class="number">
                          <h3 class="font-red-haze">
                              <span data-counter="counterup" data-value="<%= @grpah1Data7[:pageviews] %>">0</span>
                          </h3>
                          <small>PageViews</small>
                      </div>
                      <div class="icon">
                          <i class="icon-like"></i>
                      </div>
                  </div>
                  <div class="progress-info">
                      <div class="progress">
                          <span style="width: 85%;" class="progress-bar progress-bar-success red-haze">
                              <span class="sr-only">85% change</span>
                          </span>
                      </div>
                      <div class="status">
                          <div class="status-title"> change </div>
                          <div class="status-number"> 85% </div>
                      </div>
                  </div>
              </div>
          </div>
          <div class="col-lg-3 col-md-3 col-sm-6 col-xs-12">
              <div class="dashboard-stat2 ">
