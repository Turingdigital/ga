<p>目前在線人數:<%= @act_users %></p>
<table class="table table-striped table-hover ">
  <tr>
    <th>METRIC</th><th>本日</th><th>昨日</th><th>差異</th>
  </tr>
  <tr>
    <td>sessions</td><td><%= @visits.rows[-1][1] %></td>
    <td><%= @visits.rows[-2][1] %></td>
    <td><%= @visits.rows[-1][1].to_i - @visits.rows[-2][1].to_i %></td>
  </tr>
  <tr>
    <td>users</td><td><%= @visits.rows[-1][2] %></td>
    <td><%= @visits.rows[-2][2] %></td>
    <td><%= @visits.rows[-1][2].to_i - @visits.rows[-2][2].to_i %></td>
  </tr>
  <tr>
    <td>newUsers</td><td><%= @visits.rows[-1][3] %></td>
    <td><%= @visits.rows[-2][3] %></td>
    <td><%= @visits.rows[-1][3].to_i - @visits.rows[-2][3].to_i %></td>
  </tr>
  <tr>
    <td>percentNewSessions</td><td><%= @visits.rows[-1][4] %></td>
    <td><%= @visits.rows[-2][4] %></td>
    <td><%= @visits.rows[-1][4].to_i - @visits.rows[-2][4].to_i %></td>
  </tr>
  <tr>
    <td>sessionDuration </td><td><%= @visits.rows[-1][5] %></td>
    <td><%= @visits.rows[-2][5] %></td>
    <td><%= @visits.rows[-1][5].to_i - @visits.rows[-2][5].to_i %></td>
  </tr>
  <tr>
    <td>avgSessionDuration</td><td><%= @visits.rows[-1][6] %></td>
    <td><%= @visits.rows[-2][6] %></td>
    <td><%= @visits.rows[-1][6].to_i - @visits.rows[-2][6].to_i %></td>
  </tr>
</table>
<canvas id="myChart" width="400" height="400"></canvas>
<%= javascript_tag do %>
  var ctx = document.getElementById("myChart");
  var ctx = document.getElementById("myChart").getContext("2d");
  var ctx = $("#myChart");
  var myChart = new Chart(ctx);
<% end %>
